{
  "name": "vite-ssr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vavite serve",
    "build": "tsc && vavite build",
    "serve": "NODE_ENV=production node dist/server",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
